</head>

<body data-ng-app="myPage" data-ng-controller="myPageCtrl">

	<div class="wrapper">

		<%- include("partials/header", {navbar: false, anchor: false}) %>
		<section
			class="uk-height-viewport uk-cover-background uk-position-relative uk-flex uk-flex-middle uk-overflow-hidden grey_overlay container"
			style="background-image: url('/assets/sets1/images/contact.jpg')">
			<div
				class="uk-container uk-container-center uk-width-1-1 uk-text-center uk-position-relative container_block">

				<div class="container_block">
					<h1 class="uk-h1 container_title wwd-normal">Contact Us </h1>
					<h2 class="uk-h2 container_subtitle"></h2>
					<div class="container_description"> Get in touch to get the ball rolling.</div>
				</div>

			</div>
		</section>
		<section class="contact grey_overlay section">
			<div class="section-content">
				<div class="uk-container uk-container-center uk-text-center" data-uk-scrollspy="{cls:'uk-active'}">
					<h3 class="wwd-normal mid-size-text uk-margin-top-4">Get in touch</h3>
					<h4 class="uk-h4 uk-text-contrast uk-margin-large-bottom animated section_subtitle wwd-thin">To
						request analytics talent, please fill out the form below</h4>

					<form id="contact-us" action="/contact" method="POST" class="uk-form animated form_send_message" autocomplete="off" >
						<div
							class="uk-grid uk-grid-small uk-container-center uk-width-medium-2-3 uk-width-large-1-2 uk-margin-bottom">
							<div class="uk-width-small-1-1 uk-margin-bottom">
								<input id="name" name="name" class="uk-width-1-1 uk-text-contrast form_input" required type="text"
									placeholder="Name">
							</div>
							<div class="uk-width-small-1-1 uk-margin-bottom">
								<input id="company" name="company" class="uk-width-1-1 uk-text-contrast form_input" required
									type="text" placeholder="Company">
							</div>
							<div class="uk-width-small-1-1 uk-margin-bottom">
								<input id ="email" name="email" class="uk-width-1-1 uk-text-contrast form_input" required
									type="text" placeholder="E-mail">
							</div>
							<div class="uk-width-small-1-1 uk-margin-bottom">
								<input id="phone" name="phone" class="uk-width-1-1 uk-text-contrast form_input" required
									placeholder="Phone Number">
							</div>
							<div class="uk-width-small-1-1 uk-margin-bottom">
								<select id="job" name="job_type" class="uk-width-1-1 uk-text-contrast form_input" required>
									<option style="color: black" value="On-demand analytics professional">On-demand
										analytics professional</option>
									<option style="color: black" value="Full time analytics professional">Full time
										analytics professional</option>
									<option style="color: black" value="Team of analytics professionals">Team of
										analytics professionals</option>
								</select>
							</div>
							<div class="uk-margin-bottom uk-width-1-1">
								<textarea id="message" name="message" class="uk-width-1-1 uk-text-contrast form_textarea" required
									placeholder="Briefly summarize your analytics needs"></textarea>
							</div>
							<div class="uk-margin-bottom uk-width-1-1">
								<button class="uk-button button_custom_green blytix-inverse-btn form_button"
									type="submit">Contact Us</button>
							</div>
						</div>

						<div class="success">Your message has been sent successfully.</div>
						<div class="success_response"></div>

						<div class="error">E-mail must be valid and message must be longer than 1 character.</div>
						<div class="error_response"></div>
					</form>
				</div>
			</div>
		</section>

		<% include partials/footer %>

	</div>
	<div id="modal_subscribe" class="uk-modal subscribe_window">
		<div class="uk-modal-dialog uk-text-center uk-border-rounded">
			<h4 class="uk-h4 uk-text-contrast subscribe_window_title">Subscribe to our newsletter</h4>

			<form data-ng-submit="mailchimpForm" class="uk-form form_sign_in" autocomplete="off">
				<div class="uk-grid uk-grid-collapse uk-container-center uk-width-medium-3-4">
					<div class="uk-width-medium-3-4 uk-margin-bottom">
						<input name="email" class="uk-width-1-1 uk-text-contrast form_input" type="email"
							placeholder="Enter your e-mail">
					</div>
					<div class="uk-width-medium-1-4 uk-margin-bottom">
						<button class="uk-width-1-1 uk-button button_custom_green form_button"
							type="submit">Subscribe</button>
					</div>
				</div>
				<div class="response_wrapper">

					<div class="success">E-mail has been added successfully.</div>
					<div class="success_response"></div>

					<div class="error">E-mail must be valid and message must be longer than 1 character.</div>
					<div class="error_response"></div>
				</div>
			</form>
			<a class="uk-modal-close uk-close uk-border-circle subscribe_window_close"></a>
		</div>
	</div>

	<% include partials/modal %>

	<style>
		.contact {
			background-color: #da4a4a;
		}

		.blytix-inverse-btn {
			border: none;
		}
	</style>

